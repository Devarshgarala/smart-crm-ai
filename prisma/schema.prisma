datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Deal {
  id        String   @id @default(cuid())
  title     String
  value     Int
  stage     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
